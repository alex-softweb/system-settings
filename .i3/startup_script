# vim: set ft=sh:

i3 workspace 7

bash ~/.xinput.conf

guake -t &
sleep 5

if $(tmux has-session -t small); then
  guake -e 'tmx-attach-session small'
else
  guake -e 'automux small -fm'
fi

firefox &
# Wait for firefox to startup.
sleep 3
i3 [class="Firefox"] move workspace 7

touch /tmp/setup-notes-window
gnome-terminal &

clipit &
nm-applet &
bluetooth-applet &
gnome-sound-applet &
