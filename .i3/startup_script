# /usr/bin/env bash

i3 workspace 7

bash ~/.xinput.conf

guake -t &
sleep 5

if $(tmux has-session -t small); then
  guake -e 'tmx-attach-session small'
else
  guake -e 'automux small -fm'
fi

firefox &
i3 [class="Firefox"] move workspace 7

SETUP_NOTES_WINDOW=true gnome-terminal &

clipit &
nm-applet &
bluetooth-applet &
gnome-sound-applet &
