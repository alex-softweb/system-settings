# vim: set ft=sh:

i3 workspace 7

bash ~/.xinput.conf

guake -t &
sleep 5

if $(tmux has-session -t small); then
  guake -e 'tmx-attach-session small'
else
  guake -e 'automux small -fm'
fi

firefox &
sleep 10

i3-msg exec 'xfce4-terminal -e setup-notes-session'

copyq &
nm-applet &
bluetooth-applet &
gnome-sound-applet &

tmux set -g status off
edge-swarp
