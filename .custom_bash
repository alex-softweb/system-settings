# vim: set ft=sh:

. ~/.bash_prompt

stty stop undef

declare -A KEY
KEY=(
		[CTRL]="\C"
		[META]="\e"
		[ENTER]="\n"
    )

git_status="\"${KEY[CTRL]}-o\":\"gs${KEY[ENTER]}\""

bind $git_status

export EDITOR=vim

add_to_path $HOME/.rvm/bin
add_to_path $HOME/.shortcuts
add_to_path $HOME/.project_shortcuts
