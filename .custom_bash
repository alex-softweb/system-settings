# vim: set ft=sh:

. ~/.bash_aliases
. ~/.bash_functions
. ~/.bash_prompt

stty stop undef

git_status="\"${KEY[CTRL]}-o\":\"gs${KEY[ENTER]}\""

bind $git_status

export EDITOR=vim

add_to_path $HOME/.rvm/bin
add_to_path $HOME/.git_helpers
add_to_path $HOME/bin
add_to_path $HOME/.project_shortcuts

automux_installed && complete -W "$(automux blueprint list)" automux
